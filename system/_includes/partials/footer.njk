<footer role="contentinfo" class="py-5">
  <section>
    <div>
      <h5>
        <a href="/" aria-label="Site logo">
          <!-- <img src="/img/fp.svg" alt=""> -->
          <!-- <span aria-label="logo text">{{ pkg.app.name }}</span> -->
          {% include 'partials/brand.njk' %}
        </a>
        <p>
          The Sandy Foundation is seeking to uplift, inspire, and transform the lives of men, women, boys and girls. Who like their own family may be descendants of slaves..
        </p>
      </h5>
    </div>
    <div>
      <!-- <h6 id="navigate">Navigate</h6> -->
      <span>
         {% set navPages = collections.all | eleventyNavigation %}
        <ul>
          {%- for entry in navPages %}
          <li>
            <a href="{{ entry.url | url }}"{% if entry.url == page.url %} data-current="current item"{% endif %}>{{ entry.title }}</a>
          </li>
          {%- endfor %}
        </ul>
      </span>
    </div>
  </section>
  <hr>
  <section>
    <p>
      Copyright &copy; {{ hlp.11ty.year() }} {{ settings.author.name }} All rights reserved
    </p>
  </section>
  {% if env.node_env == 'development' %}
  <div style="text-align: center;">
    <p>
     NODE ENV {{env.node_env}}
    </p>
  </div>
  {% endif %}
</footer>
